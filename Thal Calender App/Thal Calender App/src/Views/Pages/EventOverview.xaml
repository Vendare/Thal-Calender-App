             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Thal_Calender_App.src.Views.Pages"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="600">
    <Grid>
        <DataGrid Margin="1" >
            <DataGrid.Columns>
                <DataGridTextColumn Header="Datum" Width="1*"/>
                <DataGridTextColumn Header="Events" Width="6*"/>
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</UserControl>
